{
  "expo": {
    "name": "OrderSeeker",
    "slug": "order-seeker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#06191C"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#06191C"
      },
      "package": "com.brunobross.orderseeker"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6b8d7a12-f4bb-4477-93ae-28af814923d6"
      }
    },
    "plugins": ["expo-localization"]
  }
}
